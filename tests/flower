import "library.svgt"

def petal()
{
  width = 10
  length = 30

  r -25
  q length -width 0
  a width 180
  q -length -width 25
  z
}

M 250 250

npetals = 22
nrings = 4

for sz=1..nrings
  for npetals
  {
    r (360/npetals)
    push
      f (sz*15)
      stomp (sz*.8) petal
    pop
  }
