import 'library.svgt'

def start()
{
  push j 10 stamp arrow pop
}

def finish()
{
  z stamp arrow
}

# angles
M 110 40 d 180

start f 100 l 90 f 50 finish

M 110 200 d 180

start f 100 r 90 f 50 finish

M 200 40 d 0

start f 100 r 90 f 50 finish

M 200 200 d 0

start f 100 l 90 f 50 finish

# vert and horiz

M 10 300 d 0

start f 50 r 90 f 50 r 90 f 50 finish

M 100 300 d 90 

start f 50 l 90 f 50 l 90 f 50 finish

M 220 300 d 90

start f 50 r 90 f 50 r 90 f 50 finish

M 300 350 d 180

start f 50 r 90 f 50 r 90 f 50 finish

# no direction (already at initial point)

M 10 400 d 0

start for 4 { f 50 r 90 } finish

M 100 400 d 0

start f 100 r 90 f 100 r 135 hb 100 100  finish
