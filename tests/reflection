import "library.svgt"

def gobbler()
{
  size = 50

  # dot gobbler
  push
  radius = (size / 2)
  eye = (radius / 8)
  r 25 f radius r 90 a radius (360 - 50) z
  pop

  up
  d -65 f (radius / 1.7)
  f eye r 90
  down
  a eye 180 a eye 180

  z
}

def heart()
{
  width = 30
  radius = (width/4)
  bottom = (width * .7)

  d -70
  a radius 170

  x = turtle.x
  y = turtle.y

  Q 0 bottom 150

  r 60

  # right hand point mirrored to left side
  Q -x y -100 

  a radius 170

  z
}

M 100 100
stamp gobbler

def lots()
{
  for v = 1..4
  {
    stomp (.5 + .3*v) { mirror gobbler }
    j 50 r 90
  }

  z
}

M 100 200
stamp lots

M 100 350
stamp {

  for v = 1..10 {
    stomp (.3+.1*v) { mirror gobbler }
    j 30 r 36
  }

  z
}

M 200 100
stamp { rotate 45 heart }
j 30 
stamp { rotate 45 { reflect 1 0 heart } }

